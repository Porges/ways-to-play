{
  "name": "11ty",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --no-source-map --watch src/sass/index.scss:public/css/index.css src/sass/print.scss:public/css/print.css",
    "watch:eleventy": "node --require esbuild-register node_modules/.bin/eleventy --serve --incremental",
    "build:sass": "sass --no-source-map src/sass/index.scss:public/css/index.css src/sass/print.scss:public/css/print.css",
    "build:eleventy": "node --require esbuild-register node_modules/.bin/eleventy",
    "build:eleventy-print": "cross-env IS_PRINT=print eleventy --output=print",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=development npm-run-all build:sass --parallel watch:*",
    "build": "cross-env NODE_ENV=production npm-run-all build:sass build:eleventy",
    "postbuild:postcss": "postcss public/css/*.css --use cssnano -r --no-map",
    "postbuild:html": "find public -iname '*.html' -exec sed -i 's%<p></p>%%g' {} \\;",
    "postbuild": "npm-run-all postbuild:*",
    "serve-built": "serve public"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@fec/eleventy-plugin-remark": "^3.0.0",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@types/node": "^18.13.0",
    "@types/slug": "^5.0.3",
    "bootstrap": "^5.1.3",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.0",
    "hast-util-has-property": "^2.0.0",
    "hast-util-heading-rank": "^2.1.0",
    "hast-util-to-string": "^2.0.0",
    "isbn3": "^1.1.33",
    "liquid-args": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "ordinal": "^1.0.3",
    "postcss": "^8.4.8",
    "postcss-cli": "^10.0.0",
    "prop-types": "^15.8.1",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-format": "^4.0.1",
    "rehype-raw": "^6.1.1",
    "remark-sectionize": "^1.1.1",
    "sass": "^1.49.9",
    "serve": "^14.0.1",
    "slug": "^8.0.0",
    "yaml": "^2.2.1"
  },
  "dependencies": {
    "esbuild-register": "^3.4.2"
  }
}
