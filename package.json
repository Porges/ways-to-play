    "@fullhuman/postcss-purgecss": "^6.0.0",
    "@jest/globals": "^29.5.0",
    "@microflash/rehype-toc": "^1.0.2",
    "@types/11ty__eleventy-img": "^4.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.9.3",
    "@types/prop-types": "^15.7.13",
    "@types/react": "^18.3.12",
