    "hast-util-heading-rank": "^3.0.0",
    "hast-util-to-string": "^3.0.0",
    "isbn3": "^1.1.44",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "jsdom": "^24.0.0",
    "liquid-args": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "ordinal": "^1.0.3",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0",
    "prop-types": "^15.8.1",
    "rehype-autolink-headings": "^7.0.0",
    "rehype-format": "^5.0.0",
