{
  "name": "11ty",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --no-source-map --watch src/sass/index.scss:public/css/index.css",
    "watch:eleventy": "eleventy --incremental --serve",
    "build:sass": "sass --no-source-map src/sass/index.scss:public/css/index.css",
    "build:eleventy": "eleventy",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=development npm-run-all build:sass --parallel watch:*",
    "build": "cross-env NODE_ENV=production npm-run-all build:sass build:eleventy",
    "postbuild:purgecss": "purgecss --css public/css/index.css --content public/**/*.html --output public/css/",
    "postbuild:postcss": "postcss public/css/*.css --use cssnano -r --no-map",
    "postbuild": "npm-run-all postbuild:purgecss postbuild:postcss",
    "serve-built": "serve public"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-img": "^1.1.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@fec/eleventy-plugin-remark": "^3.0.0",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "bootstrap": "^5.1.3",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.0",
    "hast-util-has-property": "^2.0.0",
    "hast-util-heading-rank": "^2.1.0",
    "hast-util-to-string": "^2.0.0",
    "isbn3": "^1.1.20",
    "liquid-args": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.8",
    "postcss-cli": "^9.1.0",
    "prop-types": "^15.8.1",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-format": "^4.0.1",
    "rehype-raw": "^6.1.1",
    "remark-sectionize": "^1.1.1",
    "sass": "^1.49.9",
    "serve": "^13.0.2",
    "slug": "^5.3.0"
  }
}
