    "@fec/eleventy-plugin-remark": "^4.0.0",
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "@jest/globals": "^29.5.0",
    "@microflash/rehype-toc": "^1.0.2",
    "@types/11ty__eleventy-img": "^4.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.9.1",
    "@types/prop-types": "^15.7.13",
    "@types/slug": "^5.0.9",
    "ajv": "^8.12.0",
    "bootstrap": "^5.3.0",
    "cross-env": "^7.0.3",
