    "hast-util-has-property": "^3.0.0",
    "hast-util-heading-rank": "^3.0.0",
    "hast-util-to-string": "^3.0.1",
    "isbn3": "^1.2.3",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "jsdom": "^25.0.1",
